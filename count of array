#include <stdio.h>

int main() {
    int i,max=0;
    int count=0;
    scanf("%d",&i);
    int a[i];
    for(int j=0;j<i;j++)
    {
        scanf("%d",&a[j]);
    }
    for(int k=0;k<i;k++)
    {
        if(max<=a[k])
        {
            max=a[k];
        }
    }
    for(int l=0;l<i;l++)
    {
        if(max==a[l])
        {
            count+=1;
        }
    }
    printf("%d",i-count);
    return 0;
}
